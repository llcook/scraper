{{!-- FRONT-END FOR SAVED HEADLINES --}}

<div class="container-fluid" id="wrapper">
    <h1>Wow!</h1>
    <h2><i>Saved Articles</i></h2>

    {{#each saved}}
        <h4>{{this.headline}}</h4>
        <p>{{this.description}}</p>
        <button id="unsave">Unsave</button>
        <button id="notes">Notes</button>
    {{/each}}


    <div id="saved"></div>
    <div id="notes"></div>
</div>